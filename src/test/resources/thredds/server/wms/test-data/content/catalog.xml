<?xml version="1.0" encoding="UTF-8"?>
<catalog
    name="THREDDS Server Default Catalog : You must change this to fit your server!"
    xmlns="http://www.unidata.ucar.edu/namespaces/thredds/InvCatalog/v1.0"
    xmlns:xlink="http://www.w3.org/1999/xlink">

    <service name="wms" serviceType="WMS" base="/thredds/wms/" />

    <datasetRoot path="data" location="content/data/" />

    <datasetScan name="Laamu Atoll" ID="laamu" path="laamu"
        location="content/data/laamu">
        <metadata inherited="true">
            <serviceName>wms</serviceName>
            <dataType>Grid</dataType>
        </metadata>
        <filter>
            <include wildcard="*.nc" />
        </filter>
    </datasetScan>

</catalog>
